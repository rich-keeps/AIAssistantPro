# 生产环境配置
VITE_APP_ENV=production

# API配置 - 内网IP访问
VITE_API_BASE_URL=http://192.168.1.12

# 应用配置
VITE_APP_TITLE=AI智能化管理系统
VITE_APP_DESCRIPTION=集成考勤管理、发票识别OCR、数据处理的智能管理系统

# 上传配置
VITE_MAX_UPLOAD_SIZE=52428800  # 50MB
VITE_ALLOWED_FILE_TYPES=.xlsx,.xls,.jpg,.png,.pdf

# 功能开关
VITE_ENABLE_OCR=true
VITE_ENABLE_REPORTS=true
VITE_ENABLE_SETTINGS=true

# 性能配置
VITE_ENABLE_PWA=false
VITE_ENABLE_ANALYTICS=false
